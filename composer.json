{
    "name": "clippings/parsedown-provider",
    "description": "Pimple service provider for Parsedown.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Haralan Dobrev",
            "email": "hkdobrev@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "erusev/parsedown": "^1.5.0",
        "pimple/pimple": "^3.0"
    },
    "require-dev": {
        "twig/twig": "^1.24",
        "erusev/parsedown-extra": "^0.7.1",
        "phpunit/phpunit": "^4.0,<4.5",
        "friendsofphp/php-cs-fixer": "^2.0@alpha"
    },
    "autoload": {
        "psr-4": {
            "Clippings\\ParsedownProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clippings\\ParsedownProvider\\Test\\": "tests"
        }
    },
    "suggest": {
        "erusev/parsedown-extra": "Markdown Extra support"
    }
}
